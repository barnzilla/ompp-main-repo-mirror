//LABEL(PersonCore, EN) Core functionality of the Person actor

/* NOTE(PersonCore, EN)
	This module contains the basic information which defines the Person case.
*/

actor Person //EN Individual
{
    void Start();
    void Finish();
};

void Person::Start()
{
    initialize_attributes();

    age = 0;
    time = 0;

    hook_Start();

    enter_simulation();
}

void Person::Finish()
{
    exit_simulation();
}

